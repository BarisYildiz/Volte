<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>netcoreapp3.0</TargetFramework>
        <LangVersion>preview</LangVersion>
      <StartupObject>Volte.Program</StartupObject>
        <NoWin32Manifest>true</NoWin32Manifest>
        <RuntimeIdentifiers>win10-x64</RuntimeIdentifiers>
        <RuntimeIdentifiers>linux-x64</RuntimeIdentifiers>
        <Authors>GreemDev</Authors>
        <Company>GreemDev</Company>
        <PackageProjectUrl>https://greemdev.net/Volte</PackageProjectUrl>
        <RepositoryUrl>https://github.com/GreemDev/Volte</RepositoryUrl>
        <Copyright>MIT</Copyright>
        <Version>3.0.0.0</Version>
        <Platforms>AnyCPU;x64</Platforms>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
      <PlatformTarget>x64</PlatformTarget>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
      <PlatformTarget>x64</PlatformTarget>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Colorful.Console" Version="1.2.9" />
        <PackageReference Include="Discord.Net.WebSocket" Version="2.1.1" />
        <PackageReference Include="Gommon" Version="1.1.0" />
        <PackageReference Include="Humanizer.Core" Version="2.6.2" />
        <PackageReference Include="LiteDB" Version="4.1.4" />
        <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Scripting" Version="3.3.0-beta1-final" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.0.0-preview7.19362.4" />
        <PackageReference Include="Qmmands" Version="2.3.0" />
        <PackageReference Include="RestSharp" Version="106.6.10" />
    </ItemGroup>
</Project>